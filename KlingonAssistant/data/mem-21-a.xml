    <table name="mem">
      <!-- Checked on: 2013.01.19. -->
      <column name="_id">37285</column>
      <column name="entry_name">'avwI'vaD jatlh qama'...</column>
      <column name="part_of_speech">sen:joke</column>
      <column name="definition">A prisoner says to the guard...</column>
      <column name="definition_de">Ein Gefangener sagt zur Wache...</column>
      <column name="synonyms"></column>
      <column name="antonyms"></column>
      <column name="see_also"></column>
      <column name="notes">The joke goes like this:

▶ {'avwI'vaD jatlh qama'; jIghung.}
  "A prisoner says to the guard: 'I am hungry.'"

▶ {jang 'avwI'; jIghung je.}
  "The guard replies: 'I am also hungry.'"

▶ {jatlh qama'; jI'oj.}
  "The prisoner says: 'I am thirsty.'"

▶ {jang 'avwI'; jI'oj je.}
  "The guard replies: 'I am also thirsty.'"

▶ {jatlh qama'; jIDoy'qu'.}
  "The prisoner says: 'I am very tired.'"

▶ {jatlh 'avwI'; jIDoy'be'.}
  "The guard says: 'I am not tired.'"</column>
      <column name="hidden_notes"></column>
      <column name="components">{'avwI':n}, {-vaD:n}, {jatlh:v}, {qama':n}</column>
      <column name="examples"></column>
      <column name="search_tags"></column>
      <column name="source">[1] {PK:src}</column>
    </table>
